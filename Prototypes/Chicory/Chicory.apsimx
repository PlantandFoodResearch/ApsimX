          <Name>Defoliate</Name>
          <Script>
            <removeAmountType>ResidualDM</removeAmountType>
            <cutPasture_StartDate>2012-06-10</cutPasture_StartDate>
            <cutPasture_EndDate>2013-06-10</cutPasture_EndDate>
            <interval2Cut>21</interval2Cut>
            <duration>3</duration>
            <defoliationType>Graze</defoliationType>
            <herbageAmount>200</herbageAmount>
            <fracResidue>0.05</fracResidue>
            <fracN2Remove>0.15</fracN2Remove>
            <urineDepth>300</urineDepth>
            <fracN2Dung>0.4</fracN2Dung>
            <fracPopDecline>0.2</fracPopDecline>
            <phenStageReset>3.5</phenStageReset>
          </Script>
          <Code><![CDATA[// Defoliates on a regular cycle or one-off via a schedule
// currenty defoliates by the amount specified spread evenly over the specified period

using System;
using Models.Core;
using System.Collections.Generic;
using System.Text;
using Models.Soils;
using Models.PMF;
using Models;
using System.Xml.Serialization;
using APSIM.Shared.Utilities;

namespace Models
{


	public class Defoliation
	{
		public DateTime startDate { set; get; }
		public DateTime endDate { set; get; }
		public double herbageAmount { set; get; }
	}
	

	[Serializable] 
	public class Script : Model
	{
		[Link] Plant Chicory;
		[Link] Zone zone;
		[Link] ISummary summary = null;
		[Link] Clock Clock;
		
		List<Defoliation> defoliations = new List<Defoliation>();

		//defoliations = new List<Defoliation>();
		
		//Outputs
		[XmlIgnore] private RemovalFractions removalFractions { get; set; }
		[XmlIgnore] public double LeafWt { get; set; }
		
		//Whether today is a harvesting day
		[XmlIgnore] public string Can_Cut{ get; set; }
		//Whether today's harvest was done
		[XmlIgnore] public string Has_Cut{ get; set; }
		//Pasture DM harvested today (kg/ha)
		[XmlIgnore] public double harvested_dm{ get; set; }
		//N harvested today
		[XmlIgnore] public double harvested_n{ get; set; }
		//DM removed from the paddock
		[XmlIgnore] public double removed_dm{ get; set; }
		//N removed from the paddock
		[XmlIgnore] public double removed_n{ get; set; }
		//DM returned to the paddock
		[XmlIgnore] public double returned_dm{ get; set; }
		//N returned to the paddock
		[XmlIgnore] public double returned_n{ get; set; }
		//N returned as dung (SurfaceOM)
		[XmlIgnore] public double NReturnedInDung{ get; set; }
		//N returned as urine (urea)
		[XmlIgnore] public double NReturnedInUrine{ get; set; }
		//Average growth rate for this cut
		[XmlIgnore] public double GrowthRateCalc{ get; set; }
		//Number of days after last harvest
		[XmlIgnore] public int DaysAfterCut{ get; set; }
		//Total standing DM (Leaf + Stem/Sheath) before cut
		[XmlIgnore] public double PreHarvestDM{ get; set; }
		//Total standing DM (Leaf + Stem/Sheath) after cut
		[XmlIgnore] public double PostHarvestDM{ get; set; }
		
		
		//Internal variables
		//The fraction of DM to be removed from the simulation (0-1)
		private double FracDM2Remove;
		//Whether cutting are allowed
		private bool CutIsAllowed;
		//The fraction of urine to be applied in each layer
		private double[] FracLayer;
		
		
		public enum removeAmountTypes
		{
			/// <summary>Setting DM amount to remove</summary>
			DMToRemove,
			/// <summary>Setting residual DM amount</summary>
			ResidualDM
		}


		public enum defoliationTypes
		{
			/// <summary>Cut</summary>
			Cut,
			/// <summary>Graze</summary>
			Graze
		}

		[Description("Choose what amount is being defined:")]
		public removeAmountTypes removeAmountType { get; set; }
		
		[Description("Enter the date to start the rotation (dd/mm/yyyy):")]
		public DateTime cutPasture_StartDate { get; set; }

		[Description("Enter the date to end the rotation (dd/mm/yyyy):")]
		public DateTime cutPasture_EndDate { get; set; }

		[Description("Enter the interval between harvests (days):")]
		public int interval2Cut { get; set; }

		[Description("Enter the number of days to defoliate over (days):")]
		public int duration { get; set; }

		[Description("Enter type of defoliation:")]
		public defoliationTypes defoliationType { get; set; }

		[Description("Enter the herbage amount to defoliate (kgDM/ha):")]
		public double herbageAmount { get; set; }

		[Description("Enter fraction returned as residue(0-1):")]
		public double fracResidue { get; set; }

		[Description("Enter fraction of N that is removed from the field (0-1):")]
		public double fracN2Remove { get; set; }

		[Description("Enter the depth down to which urine will applied (mm):")]
		public double urineDepth { get; set; }

		[Description("Enter the proportion of N returned in dung (0-1):")]
		public double fracN2Dung { get; set; }

		[Description("Enter the fraction of population decline due to defoliation (0-1):")]
		public double fracPopDecline { get; set; }
					  
		[Description("Enter the phenology stage that defoliating sets crop to (0=no change, or 1.0-7.0):")]
		public double phenStageReset { get; set; }

		private void defoliate(int listNumber)
		{
		
			summary.WriteMessage(this, "Defoliating (" + listNumber + ")");
			
			LeafWt = (double)zone.Get("Chicory.Leaf.Wt");
			
			double numDays = (defoliations[listNumber].endDate - defoliations[listNumber].startDate).TotalDays;
			double amountToDefoliate = defoliations[listNumber].herbageAmount / numDays;
						
			if (amountToDefoliate > LeafWt * 0.9)
			{
				amountToDefoliate = LeafWt * 0.9; 
			}			
			
			double amountToResidue = amountToDefoliate * fracResidue;
			double fractionToDefoliate = (amountToDefoliate - amountToResidue) / LeafWt;
			double fractionToResidue = amountToResidue / LeafWt;
			
			removalFractions.SetFractionRemoved("Leaf", fractionToDefoliate);
			removalFractions.SetFractionToResidue("Leaf", fractionToResidue);
			
			removalFractions.SetFractionRemoved("Root", 0.0);
			removalFractions.SetFractionToResidue("Root", 0.0);
			removalFractions.SetFractionRemoved("Stem", 0.0);
			removalFractions.SetFractionToResidue("Stem", 0.0);  
			
			if (phenStageReset > 0)
			{
				removalFractions.SetPhenologyStage = phenStageReset;
			}
			
			if (defoliationType == defoliationTypes.Cut)
			{			
				Chicory.RemoveBiomass("Cut", removalFractions);  
			}
			else
			{
				Chicory.RemoveBiomass("Graze", removalFractions);  
			}
														
			double popDeclineToday = fracPopDecline / numDays;
			Chicory.Population = Chicory.Population * (1 - popDeclineToday);						
		
		}

		public void addDefoliation(int numDays, double amount)
		{			
			
			Defoliation defoliation = new Defoliation();
			defoliation.startDate = Clock.Today;
			defoliation.endDate = Clock.Today.AddDays(numDays);
			defoliation.herbageAmount = amount;
			
			defoliations.Add(defoliation);
			
			summary.WriteMessage(this, "Added defoliation for " + numDays + " days");
			
			
			/*

			
			
			// Reduce population			
			if ((Clock.Today > popStartDate) && (Clock.Today < popEndDate))
			{
				Chicory.Population = Chicory.Population * (1 - popDecline);
			}			 */
			
			//StoverWt = (double)zone.Get("Chicory.Stover.Wt");
			//summary.WriteMessage(this, StoverWt.ToString());

			
		}

					  
		[EventSubscribe("Commencing")]
		private void OnSimulationCommencing(object sender, EventArgs e)
		{
			removalFractions = new RemovalFractions();
		}
		
		[EventSubscribe("DoManagement")]
		private void OnDoManagement(object sender, EventArgs e)
		{			
			// Add any regular defoliations to the defoliation list if they begin today
			if ((Clock.Today >= cutPasture_StartDate) && (Clock.Today <= cutPasture_EndDate))
			{
				double daysSinceStart = (Clock.Today - cutPasture_StartDate).TotalDays;
				if (daysSinceStart % interval2Cut == 0)
				{
					summary.WriteMessage(this, "Added regular defoliation starting at " + Clock.Today.ToString());
					addDefoliation(duration, herbageAmount);
				}
			}
			
			// Check the list of defoliations to see if any need running
			for (var i = defoliations.Count - 1; i >= 0; i--)
			{
				if ((defoliations[i].startDate <= Clock.Today) && (defoliations[i].endDate > Clock.Today))
				{									
					defoliate(i);
				}
			}
		}
								
		
		[EventSubscribe("DoManagementCalculations")]
		private void OnDoManagementCalculations(object sender, EventArgs e)
		{
			
		}
	}
}
	   ]]></Code>
