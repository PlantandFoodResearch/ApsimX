          <Code><![CDATA[// This script is only intended to duplicate the management in the Ruakura Chicory trial by Frank Lee

using System;
using Models.Core;
using System.Collections.Generic;
using System.Text;
using Models.Soils;
using Models.PMF;
using Models;
using System.Xml.Serialization;
using APSIM.Shared.Utilities;

namespace Models
{
	[Serializable] 
	public class Script : Model
	{
		[Link] Plant Chicory;
		[Link] Zone zone;
		[Link] ISummary summary = null;
		[Link] Clock Clock;

		[Description("Fraction of leaves removed by defoliation (0-1)")]
		public double LeafRemoved { get; set; }

		[Description("Fraction of leaves to residue (0-1)")]
		public double LeafResidue { get; set; }


		[Description("Fraction of stem removed by defoliation (0-1)")]
		public double StemRemoved { get; set; }

		[Description("Fraction of stem to residue (0-1)")]
		public double StemResidue { get; set; }


		[Description("Population decline due to defoliation (0-1)")]
		public double popDecline { get; set; }

		[Description("Population decline starting date (d/mm/yyyy)")]
		public DateTime popStartDate { get; set; }

		[Description("Population decline ending date (d/mm/yyyy)")]
		public DateTime popEndDate { get; set; }

		[XmlIgnore] private RemovalFractions removalFractions { get; set; }
		[XmlIgnore] public double LeafWt { get; set; }
		[XmlIgnore] public double LeafHarvested { get; set; }
		[XmlIgnore] public double StemWt { get; set; }
		[XmlIgnore] public double StemHarvested { get; set; }
					        
		[EventSubscribe("Commencing")]
		private void OnSimulationCommencing(object sender, EventArgs e)
		{
			removalFractions = new RemovalFractions();
		}
		
		[EventSubscribe("DoManagement")]
		private void OnDoManagement(object sender, EventArgs e)
		{
		
		}
		
		public void Defoliate()
		{			
			
			
			//removalFractions.SetPhenologyStage = 3.5;
			
			LeafWt = (double)zone.Get("Chicory.Leaf.Wt");
			StemWt = (double)zone.Get("Chicory.Stem.Wt");
			
			LeafHarvested = LeafWt * LeafRemoved;
			StemHarvested = StemWt * StemRemoved;

			removalFractions.SetFractionRemoved("Leaf", LeafRemoved);
			removalFractions.SetFractionToResidue("Leaf", LeafResidue);

			removalFractions.SetFractionRemoved("Stem", StemRemoved);
			removalFractions.SetFractionToResidue("Stem", StemResidue);  
			
			removalFractions.SetFractionRemoved("Root", 0.0);
			removalFractions.SetFractionToResidue("Root", 0.0);
			
			Chicory.RemoveBiomass("Cut", removalFractions);
						
			
			
			// Reduce population
			if ((Clock.Today > popStartDate) && (Clock.Today < popEndDate))
			{
				Chicory.Population = Chicory.Population * (1 - popDecline);
			}			
		}
		
		[EventSubscribe("DoManagementCalculations")]
		private void OnDoManagementCalculations(object sender, EventArgs e)
		{
			
		}
	}
}
	   ]]></Code>
